<service>
	<id>afcserver.exe</id>
	<name>AFC-Server</name>
	<description>Server for Client Connections and Database-Connector.</description>
	<executable>C:\Program Files\nodejs\node.exe</executable>
	<logmode>rotate</logmode>
	<argument>--harmony</argument>
	<argument>C:\Users\Patri\AppData\Roaming\npm\node_modules\node-windows\lib\wrapper.js</argument>
	<argument>--file</argument>
	<argument>C:\app\Github\AFC-Website\AFC-Server\server.js</argument>
	<argument>--log</argument>
	<argument>AFC-Server wrapper</argument>
	<argument>--grow</argument>
	<argument>0.25</argument>
	<argument>--wait</argument>
	<argument>1</argument>
	<argument>--maxrestarts</argument>
	<argument>3</argument>
	<argument>--abortonerror</argument>
	<argument>n</argument>
	<argument>--stopparentfirst</argument>
	<argument>undefined</argument>
	<stoptimeout>30sec</stoptimeout>
	<workingdirectory>C:\app\Github\AFC-Website\AFC-Server</workingdirectory>
</service>